<h4>注文履歴一覧</h4>

<table>
  <thead>
    <tr>
      <th>注文日</th>
      <th>配送先</th>
      <th>注文商品</th>
      <th>支払金額</th>
      <th>注文詳細</th>
    </tr>
  </thead>
  <tbody>
    <% @orders.each do |order| %>
    <tr>
      <td><%= order.created_at.strftime('%Y/%m/%d') %></td>
      <td><%= order.address_display %></td>
      <td><%= order.order_detail.item_id %></td>
      <td></td>
    </tr>
    <% end %>
  </tbody>
</table>