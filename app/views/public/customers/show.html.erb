<h4>マイページ</h4>

<h4>登録情報</h4>
<%= link_to "編集", customers_current_customer_edit_path(@customer.id) %>

<table class="table">
  <tbody>
    <tr>
      <th>氏名</th>
      <th><%= customer.first_name %></th>
    </tr>
    <tr>
      <th>カナ</th>
      <th><%= customer.first_name_kana %></th>
    </tr>
    <tr>
      <th>郵便番号</th>
      <th><%= customer.postal_code %></th>
    </tr>
    <tr>
      <th>住所</th>
      <th><%= customer.address %></th>
    </tr>
    <tr>
      <th>電話番号</th>
      <th><%= customer.telephone_number %></th>
    </tr>
    <tr>
      <th>メールアドレス</th>
      <th><%= customer.email %></th>
    </tr>
  </tbody>
</table>