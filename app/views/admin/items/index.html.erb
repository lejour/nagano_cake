<h4>商品一覧</h4>
<table>
  <thead>
    <tr>
      <th>商品ID</th>
      <th>商品名</th>
      <th>税抜価格</th>
    </tr>
  </thead>
  <tbody>
    <% @items.each_with_index do |item, i| %>
    <tr>
      <td><%= i + 1 %></td>
      <td><%= item.name %></td>
      <td><%= item.price %></td>
    </tr>
    <% end %>
  </tbody>
</table>

<%= paginate items %>